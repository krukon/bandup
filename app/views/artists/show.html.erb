<div class="container cover">
	<h1>cover</h1>
</div>
<div class="container artist-profile">
	<div class="row">
		<div class="span12">
			<%= render 'profile_info' %>
		</div>
	</div>
	<div class="row">
		<div class="span12">
			<h2 class="section"><i class="icon icon-group"></i>Bands</h2>
			<ul class="icons-ul">
				<li><i class="icon-li icon icon-check"></i>Band #1 (2012 - *): Electric guitar</li>
				<li><i class="icon-li icon icon-check"></i>Band #2 (2010 - *): Bass</li>
				<li><i class="icon-li icon icon-unchecked"></i>Band #3 (2007 - 2010): Vocal + Electric guitar</li>
			</ul>
		</div>
	</div>
	<div class="row">
		<div class="span12">
			<h2 class="section"><i class="icon icon-microphone"></i>Instruments &amp; Roles
				&#xf194;</h2>
			<ul class="icons-ul">
				<li><i class="icon-li icon icon-check"></i>Classical guitar</li>
				<li><i class="icon-li icon icon-check"></i>Electric guitar</li>
				<li><i class="icon-li icon icon-check"></i>Bass</li>
				<li><i class="icon-li icon icon-check"></i>Vocal</li>
			</ul>
		</div>
	</div>
	<div class="row">
		<div class="span12">
			<h2 class="section"><i class="icon icon-music"></i>Favourite genres</h2>
			<ul class="icons-ul">
				<li><i class="icon-li icon icon-check"></i>Rock</li>
				<li><i class="icon-li icon icon-check"></i>Alternative Rock</li>
				<li><i class="icon-li icon icon-check"></i>Metal</li>
				<li><i class="icon-li icon icon-check"></i>Heavy metal</li>
			</ul>
		</div>
	</div>
